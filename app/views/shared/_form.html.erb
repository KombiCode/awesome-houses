<%= simple_form_for(property) do |f| %>
  <%= f.input :title %>
  <%= f.input :category, collection: Property::CATEGORIES, class: 'form-control beautiful-dropdown', include_hidden: false %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>

  <%= f.submit %>
<% end %>
